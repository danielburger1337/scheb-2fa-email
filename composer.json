{
    "name": "danielburger1337/2fa-email",
    "description": "Extends scheb/2fa-bundle with two-factor authentication via email",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Burger",
            "homepage": "https://github.com/danielburger1337"
        }
    ],
    "homepage": "https://github.com/danielburger1337/scheb-2fa-email",
    "keywords": [
        "2fa",
        "two-factor",
        "two-step",
        "authentication",
        "symfony",
        "email",
        "scheb-2fa"
